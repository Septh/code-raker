{
  "name": "rollup-plugin-code-raker",
  "version": "0.0.1",
  "description": "A Rollup plugin that rakes your bundle to remove dead leaves.",
  "keywords": [
    "rollup",
    "vite",
    "plugin",
    "rollup-plugin",
    "vite-plugin",
    "clean",
    "cleanup",
    "console",
    "debugger",
    "comments"
  ],
  "author": "Stephan 'Septh' Schreiber <septh@sfr.fr>",
  "license": "MIT",
  "type": "module",
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "files": [
    "dist",
    "!*.map"
  ],
  "scripts": {
    "prebuild": "tsc -p tsconfig.build.json",
    "build": "rollup -c",
    "pretest": "npm run build",
    "test": "npm run testOnly",
    "testOnly": "node --import @septh/ts-run --test \"test/**/*.test.ts\""
  },
  "peerDependencies": {
    "rollup": "^4.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-virtual": "^3.0.2",
    "@septh/ts-run": "^2.0.0",
    "@types/node": "^22.8.1",
    "estree-walker": "^3.0.3",
    "magic-string": "^0.30.12",
    "rollup": "^4.24.2",
    "rollup-plugin-node-externals": "^7.1.3",
    "tslib": "^2.8.0",
    "typescript": "^5.6.3"
  }
}
